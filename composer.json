{
  "name": "mixable/cakephp-locatable",
  "description": "CakePHP behavior to add GPS location data to any table.",
  "type": "cakephp-plugin",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "cakephp/cakephp": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "Mixable\\Locatable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mixable\\Locatable\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}